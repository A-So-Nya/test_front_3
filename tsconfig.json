{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "esnext",
    "strict": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  },
  "exclude": ["webpack.config.js"],
  "include": [
    "src"
, "declarations.d.ts"  ]
}
